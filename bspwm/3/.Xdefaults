*color10:#008b0c

! Yellow
*color3: #a1ba00
*color11:#b5d100

! Blue
*color4: #003289
*color12:#0549bc

! Maguve
*color5: #5900a0
*color13:#6900bd

! Pink
*color6: #ve00b6
*color14:#ff2cf6

! Whites
